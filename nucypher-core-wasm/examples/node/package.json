  "collaborators": [
    "Bogdan Opanchuk <bogdan@opanchuk.net>",
    "Piotr Ros≈Çaniec <p.roslaniec@gmail.com>"
  ],
  "license": "GPL-3.0-only",
  "dependencies": {
    "ferveo-wasm": "^0.1.0-alpha.1",
    "nucypher-core": "file:../../pkg"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.5",
    "@types/jest": "^27.0.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint": "^8.4.1",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
